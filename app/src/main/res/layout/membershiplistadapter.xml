<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardView"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/benefitslayout"
        android:layout_width="450dp"
        android:layout_height="match_parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toTopOf="@+id/benefitslayoutL3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainbenefit"
        app:layout_constraintVertical_bias="0.0">

        <ProgressBar
            android:id="@+id/level1"
            style="?android:progressBarStyleHorizontal"
            android:layout_width="147dp"
            android:layout_height="151dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="204dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/progressbar"
            android:max="100"
            android:progress="100"
            android:progressDrawable="@drawable/circle"
            android:progressTint="@color/wallet_highlighted_text_holo_dark"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.397"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/levelview1"
            android:layout_width="52dp"
            android:layout_height="35dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:text=" أزرق"
            android:textAlignment="center"
            android:textColor="@android:color/holo_blue_dark"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/level1"
            app:layout_constraintEnd_toStartOf="@+id/textView10L1"
            app:layout_constraintHorizontal_bias="0.44"
            app:layout_constraintStart_toStartOf="@+id/level1"
            app:layout_constraintTop_toTopOf="@+id/level1"
            app:layout_constraintVertical_bias="0.551" />

        <TextView
            android:id="@+id/textView10L1"
            android:layout_width="67dp"
            android:layout_height="26dp"
            android:layout_marginEnd="100dp"
            android:text="العضوية : "
            android:textAlignment="center"
            android:textColor="@android:color/darker_gray"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/levelview1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/levelview1" />

        <TextView
            android:id="@+id/textView10L"
            android:layout_width="67dp"
            android:layout_height="26dp"
            android:layout_marginBottom="8dp"
            android:text="المميزات : "
            android:textAlignment="center"
            android:textColor="@android:color/darker_gray"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/benefits0"
            app:layout_constraintEnd_toEndOf="@+id/textView10L1"
            app:layout_constraintStart_toStartOf="@+id/textView10L1" />

        <TextView
            android:id="@+id/benefits0"
            android:layout_width="351dp"
            android:layout_height="150dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="@string/bluemembership"
            android:textAlignment="center"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/divider1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.096"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/levelview1"
            app:layout_constraintVertical_bias="1.0" />

        <View
            android:id="@+id/divider1"
            android:layout_width="379dp"
            android:layout_height="26dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="184dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.145"
            app:layout_constraintStart_toStartOf="parent" />
    </android.support.constraint.ConstraintLayout>
</android.support.v7.widget.CardView>